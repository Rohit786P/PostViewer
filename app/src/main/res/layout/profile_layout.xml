            android:layout_below="@id/geoIdol"
            android:layout_height="wrap_content"
            android:text="longitudinal :"
            android:textStyle="italic"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="6dp"
            />

        <TextView
            android:id="@+id/geoLatIdle"
            android:layout_width="wrap_content"
            android:layout_below="@+id/geoLongIdle"
            android:layout_height="wrap_content"
            android:text="latitudinal  :"
            android:textStyle="italic"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="6dp"
            />
        <TextView
            android:id="@+id/geoLongtDynamic"
            android:layout_width="wrap_content"
            android:layout_toRightOf="@id/geoLongIdle"
            android:layout_height="wrap_content"
            android:text="longitudinal"
            android:textStyle="italic"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="42dp"

            />

        <TextView
            android:id="@+id/geoLatDynamic"
            android:layout_width="wrap_content"
            android:layout_toRightOf="@+id/geoLongIdle"
            android:layout_height="wrap_content"
            android:text="latitudinal "
            android:textStyle="italic"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginTop="85dp"
            android:layout_marginLeft="5dp"
            />

