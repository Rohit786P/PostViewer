<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical">
    <!--<android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height=""></android.support.design.widget.AppBarLayout>-->

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

<RelativeLayout
    android:id="@+id/layout1"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:orientation="vertical"
    android:background="@color/colorAccent"
    >
    <ImageView
        android:id="@+id/imageOfUser"
        android:layout_width="100dp"
        android:src="@drawable/user"
        android:layout_height="100dp" />
    <TextView
        android:id="@+id/UserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:fontFamily="sans-serif-smallcaps"
        android:layout_toRightOf="@id/imageOfUser"
        android:text="Anthony Gonsalves"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="15dp"
        android:textStyle="bold"
        />

</RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/layout1"
        android:background="@color/colorPrimaryDark">
        <TextView
            android:id="@+id/userIdIdol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="User ID:"
            android:textStyle="bold"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="8dp"
            />

        <TextView
            android:id="@+id/userIdDynamic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:layout_toRightOf="@id/userIdIdol"
            android:textStyle="bold"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="6dp"
            />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout3"
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layout2"
        android:background="@color/colorPrimaryDark">
        <TextView
            android:id="@+id/userNameIdol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name :"
            android:textStyle="bold"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="8dp"
            />

        <TextView
            android:id="@+id/userNameDynamic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:layout_toRightOf="@id/userNameIdol"
            android:textStyle="bold"
            android:fontFamily="sans-serif-smallcaps"
            android:textSize="25sp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="6dp"
            />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/layout4"
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/layout3"
        android:background="@color/colorPrimaryDark">
        <TextView
